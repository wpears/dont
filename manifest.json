{
  "manifest_version": 2,
  "name": "Don't Make Me Watch",
  "description":"Keep ads from pausing when you switch to another tab",
  "version":"1.0.0",
  "icons":{"16":"16.png","48":"48.png","128":"128.png"},
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "js":["dont.js"],
      "run_at":"document_start"
    }
  ]
}
